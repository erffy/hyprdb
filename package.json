{
  "name": "hypr.db",
  "version": "6.0.0",
  "private": false,
  "description": "Faster, Lightweight and Small Advanced database module.",
  "main": "src/index.js",
  "module": "src/index.js",
  "types": "src/global.d.ts",
  "exports": {
    ".": {
      "default": "./src/index.js",
      "types": "./src/global.d.ts"
    },
    "./drivers": {
      "default": "./src/drivers/index.js",
      "types": "./src/drivers/index.d.ts"
    }
  },
  "scripts": {
    "start": "node src/index",
    "start:esm": "node src/index",
    "build": "rm -rf src && tsc && prettier --write ./src"
  },
  "keywords": [
    "db",
    "database",
    "json",
    "bson",
    "yaml",
    "hyper",
    "hypr",
    "ini",
    "json5",
    "toml"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erqeweew/hyprdb.git"
  },
  "bugs": {
    "url": "https://github.com/erqeweew/hyprdb/issues"
  },
  "homepage": "https://github.com/erqeweew/hyprdb/wiki",
  "author": "erqewee",
  "license": "Apache-2.0",
  "dependencies": {
    "kleur": "^4.1.5"
  },
  "peerDependencies": {
    "@iarna/toml": "^2.x",
    "bson": "^5.x",
    "ini": "^4.x",
    "js-yaml": "^4.x",
    "json5": "^2.x",
    "yaml": "^2.x"
  },
  "peerDependenciesMeta": {
    "@iarna/toml": {
      "optional": true
    },
    "bson": {
      "optional": true
    },
    "ini": {
      "optional": true
    },
    "js-yaml": {
      "optional": true
    },
    "json5": {
      "optional": true
    }
  },
  "devDependencies": {
    "@iarna/toml": "^2.2.5",
    "@types/ini": "^1.3.31",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.4.8",
    "bson": "^5.4.0",
    "ini": "^4.1.1",
    "js-yaml": "^4.1.0",
    "json5": "^2.2.3",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=15.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
