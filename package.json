{
  "name": "hypr.db",
  "version": "1.1.2",
  "private": false,
  "description": "Faster, Lightweight and Small Advanced database module.",
  "main": "./src/index.mjs",
  "types": "./src/global.d.ts",
  "exports": {
    ".": {
      "require": "./src/index.js",
      "import": "./src/index.mjs",
      "types": "./src/global.d.ts"
    },
    "./esm": "./src/index.mjs",
    "./cjs": "./src/index.js",
    "./esm/database": "./src/classes/Database.mjs",
    "./cjs/database": "./src/classes/Database.js",
    "./esm/json": "./src/structures/JSON.mjs",
    "./cjs/json": "./src/structures/JSON.js",
    "./esm/yaml": "./src/structures/YAML.mjs",
    "./cjs/yaml": "./src/structures/YAML.js",
    "./esm/bson": "./src/structures/BSON.mjs",
    "./cjs/bson": "./src/structures/BSON.js"
  },
  "scripts": {
    "start": "clear && node src/index",
    "start:esm": "clear && node src/index.mjs"
  },
  "keywords": [
    "db",
    "database",
    "json",
    "bson",
    "yaml",
    "hyper",
    "hypr"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/erqeweew/hyprdb.git"
  },
  "homepage": "https://github.com/erqeweew/hyprdb",
  "author": "erqewee",
  "license": "MIT",
  "dependencies": {
    "@sapphire/shapeshift": "^3.9.2",
    "graceful-fs": "^4.2.11",
    "lodash": "^4.17.21",
    "string-colorizer": "^1.0.2"
  },
  "devDependencies": {
    "@types/graceful-fs": "^4.1.6",
    "@types/node": "^20.3.3",
    "bson": "^5.3.0",
    "yaml": "^2.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}