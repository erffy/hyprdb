{
  "name": "hypr.db",
  "version": "2.0.0",
  "private": false,
  "description": "Faster, Lightweight and Small Advanced database module.",
  "main": "./src/index.mjs",
  "types": "./src/global.d.ts",
  "exports": {
    ".": {
      "require": "./src/index.js",
      "import": "./src/index.mjs",
      "types": "./src/global.d.ts"
    },
    "./esm": "./src/index.mjs",
    "./cjs": "./src/index.js",
    "./esm/database": "./src/classes/Database.mjs",
    "./cjs/database": "./src/classes/Database.js",
    "./esm/json": "./src/drivers/JSON.mjs",
    "./cjs/json": "./src/drivers/JSON.js",
    "./esm/yaml": "./src/drivers/YAML.mjs",
    "./cjs/yaml": "./src/drivers/YAML.js",
    "./esm/bson": "./src/drivers/BSON.mjs",
    "./cjs/bson": "./src/drivers/BSON.js",
    "./esm/toml": "./src/drivers/TOML.mjs",
    "./cjs/toml": "./src/drivers/TOML.js"
  },
  "scripts": {
    "start": "clear && node src/index",
    "start:esm": "clear && node src/index.mjs"
  },
  "keywords": [
    "db",
    "database",
    "json",
    "bson",
    "yaml",
    "hyper",
    "hypr"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/erqeweew/hyprdb.git"
  },
  "homepage": "https://github.com/erqeweew/hyprdb",
  "author": "erqewee",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^11.1.1",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.merge": "^4.6.2",
    "lodash.set": "^4.3.2",
    "lodash.unset": "^4.5.2"
  },
  "devDependencies": {
    "@types/node": "^20.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}